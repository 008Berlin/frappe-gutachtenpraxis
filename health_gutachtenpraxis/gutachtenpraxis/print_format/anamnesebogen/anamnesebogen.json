            <br>\n            <br>\n        </td>\n    </tr>\n    \n    <tr>\n        <td>\n            <p>\n            <b>ANAMNESE</b> &nbsp;&nbsp;&nbsp;&nbsp; O <b>E</b>&nbsp; O <b>F</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; {{ doc.name }} {{ patient.last_name }}, {{ patient.first_name }}\n            </p>\n        </td>\n    </tr>\n    \n    <tr>\n        <td>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n            <br>\n        </td>\n    </tr>\n    \n    <tr>\n        <td>\n            Drogenkonsum:&nbsp;&nbsp;O Ja &nbsp; O Nein &nbsp; O welche:\n        </td>\n    </tr>\n    \n    <tr>\n        <td>\n            Medikation:\n            <br>\n            <br>\n            <br>\n            <br>\n        </td>\n    </tr>\n    \n    <tr>\n        <td>\n            Behandelnde \u00c4rzte:\n            <br>\n        HA: <div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><br>\n        NP: <div class=letter></div><div class=letter></div><div class=letter></div><div class=letter></div><div class